            }
            for (var i = 0; i < document.getElementsByClassName("add-btn").length; i++) {
                document.getElementsByClassName("add-btn")[i].style.background = color4;
            }
        }

        function change_theme(color1, color2, color3, color4) {
            colorChange(color1, color2, color3, color4);
            if (localStorage.getItem('primary') != null && localStorage.getItem('secondary') != null) {
                localStorage.removeItem('primary');
                localStorage.removeItem('secondary');
                localStorage.removeItem('title');
                localStorage.removeItem('btn');
            }
            localStorage.setItem('primary', color1);
            localStorage.setItem('secondary', color2);
            localStorage.setItem('title', color3);
            localStorage.setItem('btn', color4);
