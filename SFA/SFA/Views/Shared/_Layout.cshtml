        function colorChange(color1, color2, color3, color4) {
            document.getElementById("topBarNav").style.background = color1;
            document.getElementById("sidebar-wraper").style.background = color2;
            for (var i = 0; i < document.getElementsByClassName("topbar").length; i++) {
                document.getElementsByClassName("topbar")[i].style.background = color3;
            }
