        }
        function font_change(fontStyle) {
            for (var i = 0; i < document.getElementsByTagName("body").length; i++) {
                document.getElementsByTagName("body")[i].style.fontFamily = fontStyle + ",sans-serif";
            }
        }
        function changeFont(fontStyle) {
            var r = font_change(fontStyle);
            if (localStorage.getItem('font_style') != null) {
                localStorage.removeItem('font_style');
            }
            localStorage.setItem('font_style', fontStyle);
            return r;
