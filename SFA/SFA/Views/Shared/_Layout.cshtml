            for (var i = 0; i < paragraph.length; i++) {
                if (paragraph[i].style.fontSize) {
                    var pointer = parseInt(paragraph[i].style.fontSize.replace("%", ""));
                } else if (localStorage.getItem("current_value") === null || localStorage.getItem("current_value") === "100") {
                    pointer = 100;
                }

