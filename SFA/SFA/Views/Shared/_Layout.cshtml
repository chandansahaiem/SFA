        if (localStorage.getItem("zoom_value") !== null && localStorage.getItem("zoom_value") !== "100") {
            var pointer = parseInt(localStorage.getItem("zoom_value"));
            var body = document.getElementsByTagName("body");
            for (var i = 0; i < body.length; i++) {
                body[i].style.zoom = pointer + "%"
            }
        }


        function zoomIn() {
            var max_zoom = 110;
            var zoom_val = "increase";
            localStorage.setItem("zoom_val", zoom_val);
            var body = document.getElementsByTagName("body");
            for (var i = 0; i < body.length; i++) {
                if (localStorage.getItem("zoom_value") !== null && localStorage.getItem("zoom_value") !== "100") {
                    var pointer = parseInt(localStorage.getItem("zoom_value"));
                }
                else {
                    if (body[i].style.zoom) {
                        var pointer = parseInt(body[i].style.zoom);
                    } else {
                        pointer = 100;
                    }
                }

                if (pointer != max_zoom) {
                    pointer += 5;
                }
                localStorage.setItem("zoom_value", pointer);
                body[i].style.zoom = pointer + "%";
            }

        }
        function zoomOut() {
            var min_zoom = 65;
            var zoom_val = "decrease";
            localStorage.setItem("zoom_val", zoom_val);
            var body = document.getElementsByTagName("body");
            for (var i = 0; i < body.length; i++) {
                if (localStorage.getItem("zoom_value") !== null && localStorage.getItem("zoom_value") !== "100") {
                    var pointer = parseInt(localStorage.getItem("zoom_value"));
                }
                else {
                    if (body[i].style.zoom) {
                        var pointer = parseInt(body[i].style.zoom);
                    } else {
                        pointer = 100;
                    }
                }

                if (pointer != min_zoom) {
                    pointer -= 5;
                }
                localStorage.setItem("zoom_value", pointer);
                body[i].style.zoom = pointer + "%";
            }
        }
        function zoomDefault() {
            var zoom_val = "default";
            localStorage.setItem("zoom_val", zoom_val);
            var body = document.getElementsByTagName("body");
            for (var i = 0; i < body.length; i++) {
                body[i].style.zoom = "100%";
            }
            localStorage.setItem("zoom_value", 100);
        }
